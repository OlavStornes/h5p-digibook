{
  "globals": {
    "H5P": true,
    "H5PIntegration": true,
    "H5PEditor": true,
    "ns": true,
    "EJS": true
  },
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "valid-jsdoc": "off",
    "semi": [2, "always"],
    "indent": ["error", 2, {"SwitchCase": 1}],
    "brace-style": ["error", "stroustrup"],
    "no-extra-boolean-cast": "off"
  }
}